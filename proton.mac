/layer/SetName 172Yb_1
/layer/SetMat G4_Mn
/layer/AddLayer 0.4e-3 mm

/layer/SetName Al_1
/layer/SetMat Aluminium
/layer/AddLayer 7e-3 mm

/layer/shift 2 mm

/layer/SetName Mn_2
/layer/SetMat G4_Mn
/layer/AddLayer 0.998e-3 mm

/layer/SetName 172Yb_2
/layer/SetMat G4_Mn
/layer/AddLayer 0.4e-3 mm

/layer/SetName Al_2
/layer/SetMat Aluminium
/layer/AddLayer 7e-3 mm

/layer/shift 2 mm

/layer/SetName Mn_3
/layer/SetMat G4_Mn
/layer/AddLayer 0.998e-3 mm

/layer/SetName 172Yb_3
/layer/SetMat G4_Mn
/layer/AddLayer 0.4e-3 mm

/layer/SetName Al_3
/layer/SetMat Aluminium
/layer/AddLayer 7e-3 mm

