/control/cout/ignoreThreadsExcept 0
/control/verbose 2
/run/verbose 1

/run/setCut 100 um
/process/em/verbose 0
/process/em/printParameters
/run/initialize


#adding layers here:

/layer/SetName Mn_1
/layer/SetMat G4_Mn
